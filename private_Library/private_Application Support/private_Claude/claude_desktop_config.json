{
  "globalShortcut": "",
  "mcpServers": {
    "apple-mcp": {
      "command": "bunx",
      "args": ["--no-cache", "apple-mcp@latest"]
    },
    "context7": {
      "command": "bunx",
      "args": ["--no-cache", "@upstash/context7-mcp"]
    },
    "desktop-commander": {
      "command": "bunx",
      "args": ["--no-cache", "@wonderwhy-er/desktop-commander"]
    },
    "sequential-thinking": {
      "command": "bunx",
      "args": ["--no-cache", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "playwright": {
      "args": ["--no-cache", "@playwright/mcp@latest"],
      "command": "bunx"
    }
  }
}
