{
  "globalShortcut": "",
  "mcpServers": {
    "apple-mcp": {
      "command": "bunx",
      "args": ["@dhravya/apple-mcp"],
      "type": "stdio"
    },
    "cloudflare": {
      "command": "bunx",
      "args": ["mcp-remote", "https://bindings.mcp.cloudflare.com/sse"],
      "type": "sse"
    },
    "context7": {
      "command": "bunx",
      "args": ["@upstash/context7-mcp"],
      "type": "stdio"
    },
    "desktop-commander": {
      "command": "bunx",
      "args": ["@wonderwhy-er/desktop-commander"],
      "type": "stdio"
    },
    "duckduckgo": {
      "command": "bunx",
      "args": ["duckduckgo-mcp-server"],
      "type": "stdio"
    },
    "playwright": {
      "command": "bunx",
      "args": ["@playwright/mcp"],
      "type": "stdio"
    },
    "sequential-thinking": {
      "command": "bunx",
      "args": ["@modelcontextprotocol/server-sequential-thinking"],
      "type": "stdio"
    }
  }
}
