{
  "globalShortcut": "",
  "mcpServers": {
    "apple-mcp": {
      "command": "bunx",
      "args": ["--no-cache", "@dhravya/apple-mcp@latest"],
      "type": "stdio"
    },
    "cloudflare": {
      "command": "bunx",
      "args": ["--no-cache", "mcp-remote", "https://bindings.mcp.cloudflare.com/sse"],
      "type": "sse"
    },
    "context7": {
      "command": "bunx",
      "args": ["--no-cache", "@upstash/context7-mcp@latest"],
      "type": "stdio"
    },
    "desktop-commander": {
      "command": "bunx",
      "args": ["--no-cache", "@wonderwhy-er/desktop-commander@latest"],
      "type": "stdio"
    },
    "playwright": {
      "args": ["--no-cache", "@playwright/mcp@latest"],
      "command": "bunx",
      "type": "stdio"
    },
    "sequential-thinking": {
      "command": "bunx",
      "args": ["--no-cache", "@modelcontextprotocol/server-sequential-thinking"],
      "type": "stdio"
    }
  }
}
