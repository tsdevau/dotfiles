  },
  {
    "key": "cmd+k alt+cmd+s",
    "command": "-git.stageSelectedRanges",
    "when": "editorTextFocus && !operationInProgress && resourceScheme == 'file'"
  },
  {
    "key": "cmd+k cmd+n",
    "command": "-git.unstageSelectedRanges",
    "when": "editorTextFocus && isInDiffEditor && isInDiffRightEditor && !operationInProgress && resourceScheme == 'git'"
  },
  {
    "key": "cmd+k cmd+r",
    "command": "-git.revertSelectedRanges",
    "when": "editorTextFocus && !operationInProgress && resourceScheme == 'file'"
  },
  {
    "key": "cmd+k cmd+p",
    "command": "-diffCommit.previewCommitMessage",
    "when": "!terminalFocus"
  },
  {
    "key": "cmd+k l",
    "command": "-prettyxml.prettifyxml",
    "when": "true"
  },
  {
    "key": "cmd+k m",
    "command": "-pr.makeSuggestion",
    "when": "commentEditorFocused"
  },
  {
    "key": "cmd+k `",
    "command": "-prettyxml.minimizexml",
    "when": "true"
  },
  {
    "key": "cmd+k v",
    "command": "-markdown.extension.closePreview",
    "when": "activeWebviewPanelId == 'markdown.preview'"
  },
  {
    "key": "cmd+k cmd+g",
    "command": "diffCommit.generateCommitMessage"
  },
  {
    "key": "cmd+k cmd+a",
    "command": "git.stageAll"
  }
]
