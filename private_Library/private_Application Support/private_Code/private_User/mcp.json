{
  "inputs": [],
  "servers": {
    "apple-mcp": {
      "args": ["@dhravya/apple-mcp"],
      "command": "bunx"
    },
    "chrome-devtools": {
      "args": "chrome-devtools-mcp@latest",
      "command": "bunx"
    },
    "cloudflare": {
      "type": "http",
      "url": "https://docs.mcp.cloudflare.com/mcp"
    },
    "console-ninja": {
      "args": ["node ~/.console-ninja/mcp/"],
      "command": "bunx"
    },
    "context7": {
      "args": ["@upstash/context7-mcp@latest"],
      "command": "bunx"
    },
    "playwright": {
      "args": ["@playwright/mcp@latest"],
      "command": "bunx"
    },
    "sequential-thinking": {
      "args": ["@modelcontextprotocol/server-sequential-thinking"],
      "command": "bunx"
    },
    "stripe": {
      "type": "http",
      "url": "https://mcp.stripe.com"
    },
    "svelte": {
      "type": "http",
      "url": "https://mcp.svelte.dev/mcp"
    },
    "wallaby": {
      "args": ["node ~/.wallaby/mcp/"],
      "command": "bunx"
    }
  }
}
