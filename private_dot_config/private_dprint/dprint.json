{
  "typescript": {
    "semiColons": "asi",
    "quoteProps": "asNeeded",
    "arrowFunction.useParentheses": "force",
    "jsx.multiLineParens": "always"
  },
  "json": {
    "lineWidth": 120,
    "trailingCommas": "never"
  },
  "markdown": {
    "lineWidth": 120,
    "emphasisKind": "underscores",
    "strongKind": "asterisks"
  },
  "toml": {},
  "dockerfile": {},
  "ruff": {},
  "malva": {},
  "markup": {},
  "yaml": {
    "printWidth": 120
  },
  "graphql": {},
  "stylua": {
    "lineWidth": 120,
    "verify": true
  },
  "excludes": ["**/node_modules", "**/*-lock.json"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.95.4.wasm",
    "https://plugins.dprint.dev/json-0.20.0.wasm",
    "https://plugins.dprint.dev/markdown-0.18.0.wasm",
    "https://plugins.dprint.dev/toml-0.7.0.wasm",
    "https://plugins.dprint.dev/dockerfile-0.3.2.wasm",
    "https://plugins.dprint.dev/ruff-0.3.9.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.12.1.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.20.0.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.1.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_graphql-v0.2.1.wasm",
    "https://plugins.dprint.dev/RubixDev/stylua-v0.2.1.wasm"
  ]
}
